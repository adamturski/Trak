<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
    <application>Trak</application>
    <version>${appengine.app.version}</version>
    <threadsafe>true</threadsafe>
    <sessions-enabled>true</sessions-enabled>

    <system-properties>
        <property name="java.util.logging.config.file" value="WEB-INF/classes/log4j.properties"/>
        <property name="cloudsql.url" value="${cloudsql.url}"/>
        <property name="cloudsql.url.dev" value="${cloudsql.url.dev}"/>
    </system-properties>
</appengine-web-app>
